{
	"hosting": {
		"public": "dist/ng-portfolio/browser",
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
		"headers": [
			{
				"source": "/**",
				"headers": [
					{
						"key": "Strict-Transport-Security",
						"value": "max-age=31536000; includeSubDomains; preload"
					}
				]
			}
		]
	}
}
